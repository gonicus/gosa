{
  "type": "widget",
  "layout": "qx.ui.layout.Grow",
  "extensions": {
    "tabConfig": {
      "title": "tr('Schreibtisch')"
    }
  },
  "class": "qx.ui.container.Composite",
  "children": [
    {
      "class": "qx.ui.container.Composite",
      "layout": "qx.ui.layout.Grid",
      "layoutConfig": {
        "spacingX": "CONST_SPACING_X",
        "spacingY": "CONST_SPACING_Y"
      },
      "extensions": {
        "layoutOptions": {
          "columnFlex": {
            "column": 1,
            "flex": 1
          }
        }
     },
      "children": [
        {
          "addOptions": {
            "row": 0,
            "column": 0
          },
          "buddyModelPath": "gotoXResolution",
          "properties": {
            "text": "tr('Resolution')"
          },
          "class": "gosa.ui.widgets.QLabelWidget"
        },
        {
          "modelPath": "gotoXResolution",
          "addOptions": {
            "row": 0,
            "column": 1
          },
          "properties": {
            "tabIndex": 1
          },
          "class": "gosa.ui.widgets.QComboBoxWidget"
        },
        {
          "addOptions": {
            "row": 1,
            "column": 0
          },
          "buddyModelPath": "gotoDefaultLanguage",
          "properties": {
            "text": "tr('Preferred language')"
          },
          "class": "gosa.ui.widgets.QLabelWidget"
        },
        {
          "modelPath": "gotoDefaultLanguage",
          "addOptions": {
            "row": 1,
            "column": 1
          },
          "properties": {
            "tabIndex": 2
          },
          "class": "gosa.ui.widgets.QComboBoxWidget"
        },
        {
          "addOptions": {
            "row": 2,
            "column": 0
          },
          "properties": {
            "text": "tr('Logon script')"
          },
          "class": "gosa.ui.widgets.QLabelWidget"
        },
        {
          "addOptions": {
            "row": 2,
            "column": 1
          },
          "properties": {
            "tabIndex": 3,
            "height": 200,
            "selectorType": "dialog",
            "multivalue": true
          },
          "class": "gosa.ui.widgets.QTableWidgetWidget",
          "extensions": {
            "guiProperties": {
              "editTitle": "tr('Edit printer user')",
              "columns": {
                "scriptName": "tr('Name')",
                "scriptPriority": "tr('Priority')",
                "scriptUserEditable": "tr('Editable')",
                "scriptLast": "tr('Last')"
              },
              "additionalAttributes": ["script"],
              "sortByColumn": "scriptPriority",
              "dialogTemplate": {
                "type": "widget",
                "layout": "qx.ui.layout.VBox",
                "class": "qx.ui.container.Composite",
                "layoutConfig": {
                  "spacing": "CONST_SPACING_Y"
                },
                "properties": {
                  "width": 800,
                  "height": 500,
                  "windowTitle": "tr('Logon script')",
                  "dialogName": "editLogonScript",
                  "cancelable": true
                },
                "children": [
                  {
                    "type": "widget",
                    "layout": "qx.ui.layout.HBox",
                    "layoutConfig": {
                      "spacing": "CONST_SPACING_X"
                    },
                    "class": "qx.ui.container.Composite",
                    "children": [
                      {
                        "buddyModelPath": "scriptName",
                        "properties": {
                          "text": "tr('Name')"
                        },
                        "class": "gosa.ui.widgets.QLabelWidget"
                      },
                      {
                        "modelPath": "scriptName",
                        "properties": {
                          "tabIndex": 1
                        },
                        "addOptions": {
                          "flex": 1
                        },
                        "class": "gosa.ui.widgets.QLineEditWidget"
                      },
                      {
                        "buddyModelPath": "scriptPriority",
                        "properties": {
                          "text": "tr('Priority')"
                        },
                        "class": "gosa.ui.widgets.QLabelWidget"
                      },
                      {
                        "modelPath": "scriptPriority",
                        "properties": {
                          "tabIndex": 2
                        },
                        "class": "gosa.ui.widgets.QSpinBoxWidget"
                      },
                      {
                        "buddyModelPath": "scriptUserEditable",
                        "properties": {
                          "text": "tr('Editable')"
                        },
                        "class": "gosa.ui.widgets.QLabelWidget"
                      },
                      {
                        "modelPath": "scriptUserEditable",
                        "properties": {
                          "tabIndex": 3
                        },
                        "class": "gosa.ui.widgets.QCheckBoxWidget"
                      },
                      {
                        "buddyModelPath": "scriptLast",
                        "properties": {
                          "text": "tr('Last script')"
                        },
                        "class": "gosa.ui.widgets.QLabelWidget"
                      },
                      {
                        "modelPath": "scriptLast",
                        "properties": {
                          "tabIndex": 3
                        },
                        "class": "gosa.ui.widgets.QCheckBoxWidget"
                      }
                    ]
                  },
                  {
                    "modelPath": "script",
                    "addOptions": {
                      "flex": 1
                    },
                    "properties": {
                      "tabIndex": 4,
                      "height": 460
                    },
                    "class": "gosa.ui.widgets.EditorWidget",
                    "extensions": {
                      "guiProperties": {
                        "minimap": {
                          "enabled": false
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
        },
        {
          "addOptions": {
            "row": 3,
            "column": 0
          },
          "buddyModelPath": "gotoDefaultPrinter",
          "properties": {
            "text": "tr('Default printer')"
          },
          "class": "gosa.ui.widgets.QLabelWidget"
        },
        {
          "modelPath": "gotoDefaultPrinter",
          "addOptions": {
            "row": 3,
            "column": 1
          },
          "properties": {
            "tabIndex": 4,
            "multivalue": false
          },
          "class": "gosa.ui.widgets.QTableWidgetWidget"
        }
      ]
    }
  ]
}