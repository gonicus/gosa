<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://www.gonicus.de/Events" elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:e="http://www.gonicus.de/Events">
    <complexType name="PluginUpdate">
    	<annotation>
    		<documentation>
    			The PluginUpdate event gets fired when a new plugin has been uploaded to the backend
    		</documentation>
    	</annotation>
        <all>
    		<element name="Namespace" type="string" maxOccurs="1" minOccurs="1"></element>
    	</all>
    </complexType>
    <element name="PluginUpdate" type="e:PluginUpdate"></element>
</schema>
